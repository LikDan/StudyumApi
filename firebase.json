{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/**",
        "run": {
          "serviceId": "study-helper",
          "region": "europe-central2"
        }
      },
      {
        "source": "/api",
        "run": {
          "serviceId": "study-helper",
          "region": "europe-central2"
        }
      }
    ]
  }
}
